{
    "env": {
    "browser": true,
    "es5": true,
    "jest": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base",
        "plugin:es5/no-es2015"
      ],
    "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
    },
    "rules": {
        "es5/no-arrow-functions": "error"
    },
    "plugins": [
        "es5"
      ]
}